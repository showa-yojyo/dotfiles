{
    "breadcrumbs.enabled": false,
    "editor.cursorBlinking": "solid",
    "editor.folding": false,
    "editor.fontFamily": "'ＭＳ ゴシック', Consolas, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.fontWeight": "500",
    "editor.letterSpacing": 1,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": false,
    "editor.rulers": [
        80
    ],
    "editor.tabCompletion": "onlySnippets",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        ".bash*": "shellscript"
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.path": "C:\\Program Files\\Git\\cmd\\git.exe",
    "git.promptToSaveFilesBeforeCommit": "always",
    "git.suggestSmartCommit": false,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "python.terminal.activateEnvironment": false,
    "search.showLineNumbers": true,
    "search.followSymlinks": false,
    "terminal.external.windowsExec": "C:\\cygwin64\\Cygwin.bat",
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.scrollback": 5000,
    "terminal.integrated.shell.windows": "C:\\cygwin64\\Cygwin.bat",
    "shellLauncher.shells.windows": [
        {
            "shell": "C:\\cygwin64\\bin\\bash.exe",
            "label": "bash (Cygwin)",
            "args": [
                "--login"
            ]
        },
        {
            "shell": "C:\\msys64\\usr\\bin\\bash.exe",
            "label": "bash (MSYS2)",
            "args": [
                "--login"
            ]
        },
        {
            "shell": "C:\\Program Files\\Git\\bin\\bash.exe",
            "label": "bash (Git)",
            "args": [
                "--login"
            ]
        },
        {
            "shell": "C:\\ProgramData\\Miniconda3\\Scripts\\ipython3.exe",
            "label": "IPython"
        },
        {
            "shell": "C:\\ProgramData\\Miniconda3\\Scripts\\jupyter.exe",
            "label": "Jupyter notebook"
        },
        {
            "shell": "C:\\Ruby26-x64\\bin\\irb.cmd",
            "label": "irb",
            "args": [
                "-r",
                "irb/completion",
                "--simple-prompt"
            ]
        },
        {
            "shell": "cmd.exe",
            "label": "cmd",
            "args": []
        },
        {
            "shell": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "label": "PowerShell"
        }
    ],
    "window.enableMenuBarMnemonics": false,
    "window.newWindowDimensions": "maximized",
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "window.zoomLevel": -1,
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#ff1493",
        "activityBarBadge.background": "#ff1493",
        "statusBar.background": "#ff1493"
    },
    "workbench.colorTheme": "Default Light+",
    "workbench.editor.tabSizing": "shrink",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": false,
        "files.trimTrailingWhitespace": false
    },
    "latex-workshop.latex.recipes": [
        {
            "name": "ptex2pdf",
            "tools": [
                "ptex2pdf"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "ptex2pdf",
            "command": "ptex2pdf",
            "args": [
                "-l",
                "-ot",
                "-kanji=utf8 -synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "markdown-preview-enhanced.enableEmojiSyntax": false,
    "markdown-preview-enhanced.breakOnSingleNewLine": false,
    "markdown-preview-enhanced.frontMatterRenderingOption": "code block"
}
