{
    "breadcrumbs.enabled": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.columnSelection": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": false,
    "editor.folding": false,
    "editor.fontFamily": "'ＭＳ ゴシック', Consolas, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.fontWeight": "500",
    "editor.letterSpacing": 0,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": false,
    "editor.rulers": [
        80
    ],
    "editor.tabCompletion": "onlySnippets",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "files.associations": {
        ".bash*": "shellscript"
    },
    "files.eol": "\n",
    "files.enableTrash": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/bin": true,
        "**/*.exe": true,
        "**/*.o": true,
        "**/*.obj": true,
        "**/*.ilk": true,
        "**/*.pdb": true,
        "**/*.tlog": true,
        "**/*.idb": true,
        "**/*.dll": true,
        "**.vs": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.promptToSaveFilesBeforeCommit": "always",
    "git.suggestSmartCommit": false,
    "html.autoClosingTags": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "_site": true
    },
    "search.showLineNumbers": true,
    "search.followSymlinks": false,
    "terminal.external.windowsExec": "C:\\cygwin64\\bin\\bash.exe",
    "terminal.integrated.automationShell.windows": "wsl.exe",
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.scrollback": 5000,
    "terminal.integrated.defaultProfile.windows": "WSL",
    "terminal.integrated.profiles.windows": {
        "WSL": {
            "path": [
              "C:\\WINDOWS\\System32\\wsl.exe"
            ],
            "args": ["~", "-d", "Ubuntu"],
            "icon": "terminal-ubuntu"
        }
    },
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "manual",
    "window.enableMenuBarMnemonics": false,
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#ff1493",
        "activityBarBadge.background": "#ff1493",
        "statusBar.background": "#ff1493"
    },
    "workbench.colorTheme": "Default Light+",
    "workbench.editor.tabSizing": "shrink",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": false,
        "files.trimTrailingWhitespace": false
    },
    "emacs-mcx.strictEmacsMove": true,
    "emacs-mcx.useMetaPrefixEscape": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "hediet.vscode-drawio.local-storage": "",
    "security.workspace.trust.untrustedFiles": "open",
    "git.ignoreMissingGitWarning": true,
    "window.zoomLevel": -1
}
