{
	"unix-style path": {
		"prefix": "unix",
		"body": "${TM_SELECTED_TEXT/[\\\\]/\\//g}",
		"description": "Replace all backslashes in selection with slashes"
	},
	"latex-display": {
		"prefix": "$$",
		"body":[
			"$$",
			"\\begin{${1:aligned}}",
			"$0",
			"\\end{${1:aligned}}",
			"$$"
		]
	},
	"latex-QQ": {
		"prefix": "\\QQ",
		"body": "\\mathbb Q",
		"description": "Rational numbers"
	},
	"latex-CC": {
		"prefix": "\\CC",
		"body": "\\mathbb C",
		"description": "Complex numbers"
	},
	"latex-FF": {
		"prefix": "\\FF",
		"body": "\\mathbb F",
		"description": "Finite body"
	},
	"latex-Sn":{
		"prefix": ["\\S_", "\\sym_"],
		"body": "\\mathfrak{S}_{$1}$0",
		"description": "A symmetric group"
	},
	"latex-set-inline-simple": {
		"prefix": "\\seti",
		"body": "\\lbrace ${1}\\rbrace$0",
		"description": "{ #1 } for inline"
	},
	"latex-set-inline-comprehensive": {
		"prefix": "\\setm",
		"body": "\\lbrace ${1}\\,\\mid\\,${2}\\rbrace$0",
		"description": "{ #1 | #2 } for inline"
	},
	"latex-set-display-simple": {
		"prefix": "\\setds",
		"body": "\\{$1\\\\}$0",
		"description": "{ #1 } for display"
	},
	"latex-set-display": {
		"prefix": "\\setd",
		"body": "\\{${1}\\,|\\,${2}\\\\}$0",
		"description": "{ #1 | #2 } for display"
	},
	"latex-set-display-tall": {
		"prefix": "\\Setd",
		"body": "\\!\\left\\{\\left. ${1} \\right| \\left. ${2} \\right.\\right\\\\}\\!$0",
		"description": "\\left\\{\\left. #1 \\right| \\left. #2 \\right.\\right\\\\} for block"
	},
	"latex-parens": {
		"prefix": ["\\paren", "\\("],
		"body": "\\left(${1}\\right)$0",
		"description": "\\left(#1\\right)"
	},
	"latex-brackets": {
		"prefix": "\\[",
		"body": "\\left[$1\\right]$0",
		"description": "\\left[#1\\right]"
	},
	"latex-base": {
		"prefix": ["\\base", "\\basis"],
		"body": "\\langle ${1} \\rangle$0",
		"description": "\\langle ... \\rangle"
	},
	"latex-abs": {
		"prefix": "\\abs",
		"body": "\\lvert ${1}\\rvert$0",
		"description": "\\lvert #1\\rvert"
	},
	"latex-Abs": {
		"prefix": "\\Abs",
		"body": "\\left\\lvert ${1}\\right\\rvert$0",
		"description": "\\left\\lvert #1\\right\\rvert"
	},
	"latex-norm": {
		"prefix": "\\norm",
		"body": "\\lVert ${1}\\rVert$0",
		"description": "\\lVert #1\\rVert"
	},
	"latex-Norm": {
		"prefix": "\\Norm",
		"body": "\\left\\lVert ${1}\\right\\rVert$0",
		"description": "\\left\\lVert #1\\right\\rVert"
	},
	"latex-acosh": {
		"prefix": "\\acosh",
		"body": "\\operatorname{arccosh}",
		"description": "\\operatorname{arccosh}"
	},
	"latex-asinh": {
		"prefix": "\\asinh",
		"body": "\\operatorname{arcsinh}",
		"description": "\\operatorname{arcsinh}"
	},
	"latex-atanh": {
		"prefix": "\\atanh",
		"body": "\\operatorname{arctanh}",
		"description": "\\operatorname{arctanh}"
	},
	"latex-fn": {
		"prefix": "fn",
		"body": "${1} \\colon ${2} \\longrightarrow ${3}",
		"description": "#1 \\colon #2 \\longto #3"
	},
	"latex-mapsfrom":{
		"prefix": "\\mapsfrom",
		"body": "\\leftarrow\\!\\shortmid",
		"description": "\\mapsfrom"
	},
	"latex-longmapsfrom":{
		"prefix": "\\longmapsfrom",
		"body": "\\longleftarrow\\!\\shortmid",
		"description": "\\longmapsfrom"
	},
	"latex-dd": {
		"prefix": "\\dd",
		"body": "\\mathrm d$1",
		"description": "\\mathrm d#1"
	},
	"latex-diag": {
		"prefix": "\\diag",
		"body": "\\operatorname{diag}",
		"description": "\\operatorname{diag}"
	},
	"latex-diff": {
		"prefix": "\\diff",
		"body": "\\frac{\\mathrm{d}$1}{\\mathrm{d}$2}",
		"description": "\\frac{\\mathrm{d} {#1}{\\mathrm{d} {#2}}"
	},
	"latex-pdiff": {
		"prefix": "\\pdiff",
		"body": "\\frac{\\partial $1}{\\partial $2}",
		"description": "\\frac{\\partial {#1}{\\partial {#2}}"
	},
	"latex-eu": {
		"prefix": "\\eu",
		"body": "\\mathrm{e}",
		"description": "\\mathrm e"
	},
	"latex-inv": {
		"prefix": "\\inv",
		"body": "^{-${1:1}}$0",
		"description": "^{-a}"
	},
	"latex-prime":{
		"prefix": "prime",
		"body": "^{\\prime}",
		"description": "^{\\prime}"
	},
	"latex-prime-double":{
		"prefix": "prpr",
		"body": "^{\\prime\\prime}",
		"description": "^{\\prime}"
	},
	"latex-operatorname":{
		"prefix": "\\oper",
		"body": "\\operatorname{$1}$0",
		"description": "\\operatorname"
	},
	"latex-aut": {
		"prefix": "\\Aut",
		"body": "\\operatorname{Aut}${2}(${1})$0",
		"description": "Automorphism group"
	},
	"latex-hom": {
		"prefix": "\\Hom",
		"body": "\\operatorname{Hom}${3}(${1}, ${2})$0",
		"description": "Homomorphism group"
	},
	"latex-id": {
		"prefix": "\\id",
		"body": "\\operatorname{id}_{$0}",
		"description": "Identity map"
	},
	"latex-Im": {
		"prefix": "\\Im",
		"body": "\\operatorname{Im}{$0}",
		"description": "Imaginary part of a complex number"
	},
	"latex-im": {
		"prefix": "\\im",
		"body": "\\operatorname{im}{$0}",
		"description": "Image of linear map"
	},
	"latex-grad": {
		"prefix": "\\grad",
		"body": "\\operatorname{grad}$0",
		"description": "Operator grad"
	},
	"latex-div": {
		"prefix": "\\div",
		"body": "\\operatorname{div}$0",
		"description": "Operator div"
	},
	"latex-rot": {
		"prefix": ["\\rot", "\\curl"],
		"body": "\\operatorname{rot}$0",
		"description": "Operator rot"
	},
	"latex-gal": {
		"prefix": "\\Gal",
		"body": "\\operatorname{Gal}(${1})$0",
		"description": "Galois group"
	},
	"latex-sgn": {
		"prefix": ["\\sgn", "\\sign"],
		"body": "\\operatorname{sgn}$0",
		"description": "Signature of permutation/substitution"
	},
	"latex-sequence":{
		"prefix": ["array", "sequence", "series"],
		"body": "${1:a}_${2:1}, \\dotsc, ${1:a}_${3:n}$0",
		"description": "A sequence with indices"
	},
	"latex-spade": {
		"prefix": "spade",
		"body": "\\tag*{$\\spadesuit${1}$}$0",
		"description": "A tag for display block"
	},
	"latex-diffgeom-formindices": {
		"prefix": "\\formindices",
		"body": "${1:i}_${2:1} \\dotso ${1:i}_${3:n}",
		"description": "i_1 i_2 ... i_n"
	},
	"latex-diffgeom-ww": {
		"prefix": "\\ww",
		"body": "\\mathrm d${1:x} \\wedge \\mathrm d${2:y}",
		"description": "dx \\wedge dy"
	},
	"latex-diffgeom-formbase": {
		"prefix": "\\formbase",
		"body": "\\mathrm d${1:x}_${2:1} \\wedge \\dotsb \\wedge \\mathrm d${1:x}_${3:n}$0",
		"description": "wedge ... wedge"
	}
}
